<resources>
    <declare-styleable name="WorkoutCardView">
        <attr name="title" format="string" />
        <attr name="subTitle" format="string" />
        <attr name="setIcon" format="string" />
    </declare-styleable>
    <declare-styleable name="SelectorItem">
        <attr name="selectedIcon" format="integer" />
        <attr name="deselectedIcon" format="integer" />
        <attr name="text" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomCheckbox">
        <attr name="textLabel" format="string" />
        <attr name="backgroundWidth" format="dimension" />
    </declare-styleable>
</resources>
